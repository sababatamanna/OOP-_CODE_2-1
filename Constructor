#include<iostream>
using namespace std;

class Student
{
    public:
    int id;
    double cgpa;
    void display()
    {
        cout<<id<<" "<<cgpa<<endl;
    }
    Student(int x,double y)
    {
        id=x;
        cgpa=y;
    }
};

int main()
{
    Student Sababa(2202149,3.71);
    Sababa.display();

    Student Suparna(2202144,3.44);
    Suparna.display();
}
