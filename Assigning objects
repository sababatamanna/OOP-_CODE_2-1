#include<iostream>
using namespace std;
class Myclass
{
    int a,b;
public:
    void set(int i,int j)
    {
        a=i;b=j;
    }
    void show()
    {
        cout<< a <<' '<< b <<"\n";
    }
};
int main()
{
    Myclass o1,o2;
    o1.set(10,4);
    o2=o1;
    o1.show();
    o2.show();
    return 0;

}
